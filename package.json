{
  "name": "npm-babel-eslint-jasmine-boilerplate",
  "version": "1.0.0",
  "description": "A blank NPM module project using Babel for transpilation, ESLint for linting, and Jasmine for specs.",
  "main": "dist/promise-poller.js",
  "scripts": {
    "compile": "babel -d dist src",
    "clean": "rm -rf dist",
    "lint": "eslint src",
    "test": "npm run compile && jasmine JASMINE_CONFIG_PATH=jasmine.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/joeattardi/npm-babel-eslint-jasmine-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "scaffold",
    "project",
    "npm",
    "babel",
    "eslint",
    "jasmine"
  ],
  "author": "Joe Attardi <joe@attardi.net> (http://www.thinksincode.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joeattardi/npm-babel-eslint-jasmine-boilerplate/issues"
  },
  "homepage": "https://github.com/joeattardi/npm-babel-eslint-jasmine-boilerplate",
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "eslint": "^2.1.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine-core": "^2.4.1"
  }
}
